profile default {
	output 'GIGA-BYTE TECHNOLOGY CO., LTD. M27Q 23250B002960' mode 2560x1440@170Hz
}
profile laptop_m27q {
	output 'GIGA-BYTE TECHNOLOGY CO., LTD. M27Q 23250B002960' mode 2560x1440@143.856Hz
	output eDP-1 disable
}

profile laptop_external_fallback {
	output DP-2 enable
	output eDP-1 disable
}
profile laptop_external_fallback_bugged_DP_1 {
	output DP-2 enable
	output DP-1 disable
	output eDP-1 disable
}

profile laptop_only {
	output DP-1 disable
	output eDP-1 enable
}
